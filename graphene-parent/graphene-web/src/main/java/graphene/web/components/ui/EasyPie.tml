


<!DOCTYPE html [<!ENTITY nbsp "&#160;"> <!ENTITY copy "&#169;">  <!ENTITY bull "&#8226;"> <!ENTITY lt "&#60;"><!ENTITY gt "&#62;"> ]>

<t:container xmlns="http://www.w3.org/1999/xhtml"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" xmlns:p="tapestry:parameter">


	<div class="easy-pie-chart ${pieStyle}" data-percent="${dataPercent}"
		data-pie-size="${dataPieSize}" data-size="${dataSize}">
		<span class="percent percent-sign">0</span>
	</div>
	<span class="easy-pie-title">
		${title}
		<t:if test="showIcon">
			<i class="${icon}"></i>
		</t:if>
	</span>

</t:container>