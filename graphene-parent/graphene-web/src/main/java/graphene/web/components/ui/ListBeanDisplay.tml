
<div class="col-md-4"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd">
	<t:if test="validList">
		<div class=" well">
			<t:if test="listName">
				<h3>${listName} (${listOfThings.size()})</h3>
			</t:if>
		</div>
		<t:loop t:source="listOfThings" t:value="currentThing">
			<t:graphene.ui.meaningfulbeandisplay object="currentThing" />
		</t:loop>

	</t:if>
</div>
